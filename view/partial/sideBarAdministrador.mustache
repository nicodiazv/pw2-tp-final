    <a href="/inicioAdministrador" class="badge badge-info">Ir al inicio</a>

    <!-- Clima de google -->
    <div class="">
        {{#climaActual}}
            <h3>{{ubicacion}}</h3>
            <h3>{{descripcion}}</h3>
            <h3>{{temperatura}}</h3>
            <img src="{{icono}}">
        {{/climaActual}}
    </div>
    <!-- End Clima de google -->
    <button onclick="document.getElementById('demo').play()" class="btn btn-primary mb-2"><i class="icofont-play"></i></button>
    <button onclick="document.getElementById('demo').pause()" class="btn btn-primary mb-2"><i class="icofont-ui-pause"></i></button>
    <button onclick="document.getElementById('demo').volume+=0.1" class="btn btn-primary mb-2"><i class="icofont-volume-up"></i></button>
    <button onclick="document.getElementById('demo').volume-=0.1" class="btn btn-primary mb-2"><i class="icofont-volume-down"></i></button>
</div>
<!-- End Sidebar -->